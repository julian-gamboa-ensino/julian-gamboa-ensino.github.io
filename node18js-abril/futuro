    @GetMapping(value = "/lista", produces = MediaType.APPLICATION_JSON_VALUE)
    @ResponseBody 
    public String lista() throws FileNotFoundException {
        File folder= ResourceUtils.getFile("classpath:images/");
        listFilesForFolder(folder);
	    return "abril 18";
    }

    public void listFilesForFolder(final File folder) {
        for (final File fileEntry : folder.listFiles()) {
            if (fileEntry.isDirectory()) {
                listFilesForFolder(fileEntry);
            } else {
                System.out.println("----------"+fileEntry.getName());
            }
        }
    } 

------------------------------------------------------------------------------------------------
@ResponseBody 
public String lista() throws FileNotFoundException {
    File folder= ResourceUtils.getFile("classpath:images/");
    
    
    //return "{\"abril\":12}";
    //return "[\"https://cv-julian-2022.s3.us-west-2.amazonaws.com/certificados/00BB988B.jpg\"]";
    

    
    
    return "["+listFilesForFolder(folder)+"\"\"]";
}

public String listFilesForFolder(final File folder) {

    String saidaListaImages="";
    
    final String baseUrl = 
    ServletUriComponentsBuilder.fromCurrentContextPath().build().toUriString();

    for (final File fileEntry : folder.listFiles()) {
        if (fileEntry.isDirectory()) {
            listFilesForFolder(fileEntry);
        } else {
            saidaListaImages+="\""+baseUrl+"/image/"+fileEntry.getName()+"\",";
            //System.out.println("[\""+baseUrl+"/image/"+fileEntry.getName()+"\"]");
        }
    }

    //System.out.println(saidaListaImages);
    return saidaListaImages;
} 

